rObject* => primRObject()
stringarray* => stringArray()

#include "callable.h"

#ifdef HAVE_ROOT
#include "root.h"
#endif

typedef array stringarray;
using types::stringArray;

using namespace vm;


rObject* :nullRObject()
{
	return new rObject();
}

rObject* :rObject_Copy_helper(rObject *o)
{
	return o->Copy();
}

callable* :rObject_Copy(rObject *o)
{
	return new thunk(new bfunc(rObject_Copy_helper), o);
}

rObject *rGetObj(string file, string name, bool error=true, bool search=true)
{
	//printf("ASY_FILE:%s\n", file.c_str());
	return rObject::GetFromFile(file, name, error, search);
}

bool rObjectValid:rObjectValid(rObject *o)
{
	return o->IsValid();
}

void :rObject_Print_helper(rObject *o)
{
	o->Print();
}

callable* :rObject_Print(rObject *o)
{
	return new thunk(new bfunc(rObject_Print_helper), o);
}

void write(rObject *o)
{
	o->Write();
}

void write(string str, rObject *o)
{
	printf("%s", str.c_str());
	o->Write();
}

bool :rObject_InheritsFrom_helper(string className, rObject *o)
{
	return o->InheritsFrom(className);
}

callable* :rObject_InheritsFrom(rObject *o)
{
	return new thunk(new bfunc(rObject_InheritsFrom_helper), o);
}

stringarray *rGetListOfDirectories(string file, string baseDir)
{
	return rObject::GetListOf(file, baseDir, true, false);
}

stringarray *rGetListOfObjects(string file, string baseDir)
{
	return rObject::GetListOf(file, baseDir, false, true);
}

stringarray *rGetListOfEntries(string file, string baseDir)
{
	return rObject::GetListOf(file, baseDir, true, true);
}

